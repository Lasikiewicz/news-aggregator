rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: TEMPORARY DIAGNOSTIC RULE - NOT SECURE FOR PRODUCTION
    // This rule allows anyone to read any document in your database.
    // This is ONLY to test if data fetching works without authentication.
    match /{document=**} {
      allow read: if true;
      allow write: if false;
    }
  }
}
